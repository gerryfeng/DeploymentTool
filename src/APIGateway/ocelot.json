{
  "Routes": [
    {
      "DownstreamPathTemplate": "/Publish/OMS/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8082
        }
      ],
      "UpstreamPathTemplate": "/OMS/{url}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "Key": "OMS"
    },
    {
      "DownstreamPathTemplate": "/CityInterface/rest/services/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "127.0.0.1",
          "Port": 8082
        }
      ],
      "UpstreamPathTemplate": "/CityServer/{url}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "Key": "CityServer"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8082/Publish/GateWay"
  }
}